{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "framework": "create-react-app",
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
